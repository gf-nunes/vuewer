{"ast":null,"code":"import { api } from '~api';\nexport default {\n  props: ['repo'],\n  data: () => ({\n    issues: [],\n    loading: false,\n    temmais: false,\n    currentPage: 1\n  }),\n  methods: {\n    async listaIssues() {\n      this.loading = true;\n      const maisissues = await api.listaIssues(this.repo.owner.login, this.repo.name, this.currentPage);\n      this.issues = this.issues.concat(maisissues);\n      this.currentPage++;\n      this.loading = false;\n      this.temmais = maisissues.length > 0;\n    }\n  },\n  watch: {\n    repo() {\n      this.issues = [];\n      if (this.repo) {\n        this.temmais = false;\n        this.currentPage = 1;\n        this.listaIssues();\n      } else {\n        this.issues = [];\n        this.temmais = false;\n        this.currentPage = 1;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA;AAEA;EACAA;EACAC;IACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;MACA;MACA;MACA;MACA;MACA;MACA;IACA;EACA;EACAC;IACAC;MACA;MACA;QACA;QACA;QACA;MACA;QACA;QACA;QACA;MACA;IACA;EACA;AACA","names":["props","data","issues","loading","temmais","currentPage","methods","watch","repo"],"sourceRoot":"src/components","sources":["GithubIssues.vue"],"sourcesContent":["<template>\n    <div>\n      <v-row>\n        <v-col cols=\"12\">\n            <v-simple-table>\n              <template v-slot:default>\n                <thead>\n                  <tr>\n                    <th class=\"text-left\">Number</th>\n                    <th class=\"text-left\">Title</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"issue in issues\" :key=\"issue.number\">\n                    <td>{{ issue.number }}</td>\n                    <td>{{ issue.title }}</td>\n                  </tr>\n                </tbody>\n              </template>\n          </v-simple-table>\n        </v-col>\n      </v-row>\n      <v-row>\n        <v-col cols=\"12\">\n          <v-progress-circular indeterminate color=\"primary\" v-if=\"loading\"></v-progress-circular>\n          <v-btn color=\"primary\" v-if=\"temmais\" @click=\"listaIssues\">MAIS</v-btn>\n        </v-col>\n      </v-row>\n    </div>\n  </template>\n  \n  <script>\n  \n    import {api} from '~api'\n  \n    export default {\n      props: ['repo'],\n      data: () => ({\n        issues: [],\n        loading: false,\n        temmais: false,\n        currentPage: 1\n      }),\n      methods: {\n        async listaIssues(){\n          this.loading = true\n          const maisissues = await api.listaIssues(this.repo.owner.login, this.repo.name, this.currentPage)\n          this.issues = this.issues.concat(maisissues)\n          this.currentPage++\n          this.loading = false\n          this.temmais = maisissues.length > 0\n        }\n      },\n      watch: {\n        repo(){\n          this.issues = []\n          if (this.repo) {\n            this.temmais = false\n            this.currentPage = 1\n            this.listaIssues()\n          } else {\n            this.issues = []\n            this.temmais = false\n            this.currentPage = 1\n          }\n        }\n      }\n    }\n  </script>\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}